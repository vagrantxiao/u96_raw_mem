				<!-- <user_parameter name="CONFIG.SMON_ENABLE_INT_VOLTAGE_MONITORING" value="1"/>									 -->
				<!-- <user_parameter name="CONFIG.SMON_INTERFACE_TO_USE" value="I2C"/>			 -->
				
