                <user_parameter name="CONFIG.PS_PCIE_EP_RESET2_IO" value="PMC_MIO 39"/>                	
				<user_parameter name="CONFIG.PS_I2CSYSMON_PERIPHERAL_IO" value="PMC_MIO 18 .. 20"/>
				
            </user_parameters>
		</ip>
	</ip_preset>	


